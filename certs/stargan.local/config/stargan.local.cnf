[ req ]
default_bits       = 4096
default_md         = sha512
default_keyfile    = starganCA.pem
prompt             = no
encrypt_key        = yes
string_mask         = utf8only

# base request
distinguished_name = req_distinguished_name

# distinguished_name
[ req_distinguished_name ]
countryName            = FR                     # C=
stateOrProvinceName    = stargan                 # ST=
localityName           = stargan                 # L=
postalCode            =  75016                # L/postalcode=
streetAddress         = stargan  # L/street=
organizationName       = stargan# O=
organizationalUnitName = keamanan          # OU=
commonName            = stargan # CN=
emailAddress          = webmaster@stargan.local  # CN/emailAddress=
